                    "type": "string",
                    "default": ""
                },
                "cId": {
                    "type": "string",
                    "default": ""
                },
                "productsPerPage": {
                    "type": "number",
                    "default": 9
                },
                "stockStatus": {
                    "type": "array",
                    "default": [
                        "instock",
                        "outofstock",
                        "onbackorder"
                    ]
                },
                "selectedCategories": {
                    "type": "array",
                    "default": []
                },
                "columns": {
                    "type": "object",
                    "default": {
                        "desktop": 3,
                        "tablet": 2,
                        "mobile": 1
                    }
                },
                "columnGap": {
                    "type": "string",
                    "default": "30px"
                },
                "rowGap": {
                    "type": "string",
                    "default": "40px"
                },
                "isImage": {
                    "type": "boolean",
                    "default": true
                },
                "isTitle": {
                    "type": "boolean",
                    "default": true
                },
                "isRating": {
                    "type": "boolean",
                    "default": false
                },
                "isPrice": {
                    "type": "boolean",
                    "default": true
                },
                "isAddToCartBtn": {
                    "type": "boolean",
                    "default": true
                },
                "textAlign": {
                    "type": "string",
                    "default": "left"
                },
                "productBG": {
                    "type": "object",
