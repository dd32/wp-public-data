            "apiVersion": "1",
            "name": "wpp/popular-products",
            "title": "WooCommerce Popular Products",
            "description": "Display WooCommerce Popular Products",
            "category": "widgets",
            "keywords": [
                "woocommerce",
                "popular products",
                "best selling products"
            ],
            "textdomain": "popular-products",
            "attributes": {
